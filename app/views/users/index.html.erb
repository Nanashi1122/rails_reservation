<h3>ユーザー情報一覧</h3>

<table>
  <thead>
    <tr>
      <th>id</th>
      <th>名前</th>
      <th>メールアドレス</th>
      <th>一言紹介</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.profile %></td>
      </tr>
    <% end %>
  </tbody>
</table>